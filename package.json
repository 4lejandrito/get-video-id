    "retrieve",
    "fetch",
    "parse",
    "extract",
    "get"
  ],
  "dependencies": {
    "get-src": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "ava": "^0.12.0",
    "coveralls": "^2.13.3",
    "nyc": "^6.1.1",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "xo": "^0.12.1"
  }
}
